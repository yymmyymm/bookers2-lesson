<h1>Books#index</h1>
<p>Find me in app/views/books/index.html.erb</p>

<%= render 'form', book: @book %>

<% @books.each do |book| %>
<div>

  <p><%= image_tag 'no_image.jpg' %></p>
  <p><%= book.user.name %></p>
  <p><%=link_to book.title, book %></p>
  <p><%= book.caption %></p>

</div>
<% end %>
